{
    "name": "leetcode-tools",
    "version": "1.8.3",
    "description": "A cli tool to enjoy leetcode!",
    "bin": {
        "lc": "dist/index.js"
    },
    "files": [
        "dist/",
        "LICENSE",
        "README.md"
    ],
    "scripts": {
        "prepare": "ts-patch install -s",
        "build": "rm -rf dist && tsc -p tsconfig.json",
        "start": "ts-node ./src",
        "lint": "eslint src --ext .ts",
        "lc": "node ./dist",
        "test": "mocha -r ts-node/register test/**/*.ts --timeout 10000"
    },
    "repository": {
        "type": "git",
        "url": "git+https://github.com/Dup4/leetcode-tools.git"
    },
    "keywords": [
        "leetcode",
        "tools",
        "cli"
    ],
    "author": "Dup4",
    "license": "MIT",
    "bugs": {
        "url": "https://github.com/Dup4/leetcode-tools/issues"
    },
    "homepage": "https://github.com/Dup4/leetcode-tools#readme",
    "devDependencies": {
        "@types/fs-extra": "^9.0.13",
        "@types/js-yaml": "^4.0.5",
        "@types/mocha": "^9.1.0",
        "@types/shelljs": "^0.8.11",
        "@typescript-eslint/eslint-plugin": "^5.10.2",
        "@typescript-eslint/parser": "^5.10.2",
        "eslint": "^8.8.0",
        "eslint-config-google": "^0.14.0",
        "mocha": "^9.2.0",
        "ts-node": "^10.4.0",
        "ts-patch": "^2.0.1",
        "typescript": "^4.5.5",
        "typescript-transform-paths": "^3.3.1"
    },
    "dependencies": {
        "chalk": "4.1.0",
        "commander": "^9.0.0",
        "dotenv": "^16.0.0",
        "js-yaml": "^4.1.0",
        "leetcode-api-typescript": "^1.9.3",
        "shelljs": "^0.8.5"
    }
}
